{#
/**
 * @file
 * Default theme implementation for a paragraphs browser wrapper.
 *
 * @ingroup themeable
 */
#}

<fieldset class="paragraphs-browser-paragraph-type">
    <legend>{{ form.label }}</legend>

    {% if form.image_path %}
        <img src="{{ form.image_path }}" alt="{{ form.label }}">
    {% endif %}

    {% if form.description %}
        <span>{{ form.description }}</span>
    {% endif %}

    {{ form.add_more }}
</fieldset>